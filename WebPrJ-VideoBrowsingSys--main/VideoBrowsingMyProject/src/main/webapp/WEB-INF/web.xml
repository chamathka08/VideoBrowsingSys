<?xml version="1.0" encoding="UTF-8"?>


<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
		 
		 <servlet>
		 	<servlet-name>Login</servlet-name>
		 	<servlet-class>com.Customer.LoginServlet</servlet-class>
		 </servlet>
		 <servlet-mapping>
		 	<servlet-name>Login</servlet-name>
		 	<url-pattern>/log</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>customerinsert</servlet-name>
		 	<servlet-class>com.Customer.CustomerInsert</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>customerinsert</servlet-name>
		 	<url-pattern>/insert</url-pattern>
		 </servlet-mapping>
		 
		  <servlet>
		 	<servlet-name>UpdateCustomer</servlet-name>
		 	<servlet-class>com.Customer.UpdateCustomerServlet</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>UpdateCustomer</servlet-name>
		 	<url-pattern>/update</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>DeleteCustomer</servlet-name>
		 	<servlet-class>com.Customer.DeleteCustomerServltet</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>DeleteCustomer</servlet-name>
		 	<url-pattern>/delete</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		    <servlet-name>CreateAd</servlet-name>
		    <servlet-class>com.admin.createadServlet</servlet-class>
		  </servlet>
		  
		  <servlet-mapping>
		    <servlet-name>CreateAd</servlet-name>
		    <url-pattern>/done</url-pattern>
		  </servlet-mapping>
		  
		   <servlet>
		    <servlet-name>ReadAdForm</servlet-name>
		    <servlet-class>com.admin.readadServlet</servlet-class>
		  </servlet>
		  
		  <servlet-mapping>
		    <servlet-name>ReadAdForm</servlet-name>
		    <url-pattern>/read</url-pattern>
		  </servlet-mapping>
		  
		  <servlet>
		 	<servlet-name>UpdateAdvertiser</servlet-name>
		 	<servlet-class>com.admin.UpdateAdvertiserServlet</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>UpdateAdvertiser</servlet-name>
		 	<url-pattern>/updatead</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>DeleteAdvertiser</servlet-name>
		 	<servlet-class>com.admin.DeleteAdvertiserServlet</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>DeleteAdvertiser</servlet-name>
		 	<url-pattern>/deletead</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		    <servlet-name>Createfd</servlet-name>
		    <servlet-class>com.feedback.createfeedbackServlet</servlet-class>
		  </servlet>
		  
		  <servlet-mapping>
		    <servlet-name>Createfd</servlet-name>
		    <url-pattern>/ok</url-pattern>
		  </servlet-mapping>
		  
		   <servlet>
		    <servlet-name>Readfd</servlet-name>
		    <servlet-class>com.feedback.readfeedbackServlet</servlet-class>
		  </servlet>
		  
		  <servlet-mapping>
		    <servlet-name>Readfd</servlet-name>
		    <url-pattern>/fd</url-pattern>
		  </servlet-mapping>
		  
		  <servlet>
		 	<servlet-name>Updatefeedback</servlet-name>
		 	<servlet-class>com.feedback.UpdateFeedbackServlet</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>Updatefeedback</servlet-name>
		 	<url-pattern>/updatefd</url-pattern>
		 </servlet-mapping>
		 
		 <servlet>
		 	<servlet-name>DeleteFeedback</servlet-name>
		 	<servlet-class>com.feedback.DeleteFeedbackServlet</servlet-class>
		 </servlet>
		  <servlet-mapping>
		 	<servlet-name>DeleteFeedback</servlet-name>
		 	<url-pattern>/deletefd</url-pattern>
		 </servlet-mapping>
		  
		  <servlet>
		<servlet-name>writerInsert</servlet-name>
		<servlet-class>com.writer.writerinsertservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>writerInsert</servlet-name>
		<url-pattern>/insertWriter</url-pattern>
	</servlet-mapping>  
	
	<!-- view -->  
         <servlet>
		<servlet-name>writerview</servlet-name>
		<servlet-class>com.writer.writerviewservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>writerview</servlet-name>
		<url-pattern>/viewwriter</url-pattern>
	</servlet-mapping>  
	
	<!-- update -->  
         <servlet>
		<servlet-name>writerupdate</servlet-name>
		<servlet-class>com.writer.writerupdateservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>writerupdate</servlet-name>
		<url-pattern>/updatewriter</url-pattern>
	</servlet-mapping>  
	
	<!-- delete -->  
         <servlet>
		<servlet-name>writerdelete</servlet-name>
		<servlet-class>com.writer.writerdeleteservlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>writerdelete</servlet-name>
		<url-pattern>/deletewriter</url-pattern>
	</servlet-mapping> 
	
	
		  

</web-app>

